genGraph <- function(file){
data=read.csv(file,sep="\t")
data=data[order(data$num),]
attach(data)
lm1<-lm(time ~ length + I(length**3))
print(lm1)
trendline(length, time, model='power3P',
main="length vs time y=a*x^b+c",
xlab="length",ylab="time",pch=20)
detach(data)
}
